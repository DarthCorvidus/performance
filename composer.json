{
	"minimum-stability": "dev",
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/DarthCorvidus/plibv4-longeststring",
			"no-api": true
		},
		{
			"type": "vcs",
			"url": "https://github.com/DarthCorvidus/plibv4-vtc",
			"no-api": true
		},
		{
			"type": "vcs",
			"url": "https://github.com/DarthCorvidus/plibv4-terminaltable",
			"no-api": true
		},
		{
			"type": "vcs",
			"url": "https://github.com/DarthCorvidus/plibv4-profiler",
			"no-api": true
		}
	],
	"name": "crow-protect/crow-protect",
	"require": {
		"plibv4/longeststring": "dev-main",
		"plibv4/vtc": "dev-main",
		"plibv4/terminaltable": "dev-main",
		"plibv4/profiler": "dev-main"
	},
	"autoload": {
        "classmap": ["src/include/"]
    },
	"scripts": {
		"test": "./vendor/bin/phpunit --bootstrap  tests/autoload-composer.php tests/",
		"testdox": "./vendor/bin/phpunit --bootstrap tests/autoload-composer.php --testdox --colors=always tests/"
	},
	"config": {
		"platform": {
			"php": "7.4"
		}
	}
}
